{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "interface-name": [
            false
        ],
        "prefer-const": true,
        "no-var-requires": false,
        "no-string-literal": false,
        "object-literal-sort-keys": false,
        "no-bitwise": false,
        "forin": false,
        "max-line-length": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "no-console": [
            false
        ]
    }
}