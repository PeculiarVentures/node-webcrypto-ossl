{
    "compilerOptions": {
        "target": "esnext",
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "buildjs",
        "removeComments": true,
        "declaration": false,
        "sourceMap": false,
        "importHelpers": true,
        "noImplicitAny": true
    },
    "exclude": [
        "node_modules",
        "test"
    ]
}